                               <TableColumn fx:id="tbc_Cedula" prefWidth="87.0" text="Cédula Cliente" />
                               <TableColumn fx:id="tbc_Nombre" prefWidth="90.0" text="Nombre" />
                               <TableColumn fx:id="tbc_Telefono" prefWidth="84.0" text="Teléfono" />
