                           <font>
                               <Font name="System Bold" size="18.0" />
                           </font>
                       </Label>
   
                       <TextField fx:id="txt_Cedula" layoutX="20.0" layoutY="60.0" prefHeight="30.0" prefWidth="140.0" promptText="Cédula Cliente" />
