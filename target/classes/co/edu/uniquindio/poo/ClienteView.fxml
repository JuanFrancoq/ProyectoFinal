                       <Button fx:id="btn_LimpiaReservas" layoutX="301.0" layoutY="227.0" mnemonicParsing="false" onAction="#Limpiar" prefHeight="25.0" prefWidth="75.0" text="Limpiar" />
                       <ImageView fitHeight="202.0" fitWidth="332.0" layoutX="408.0" layoutY="48.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                               <Image url="@logoUQ.png" />
                           </image>
                       </ImageView>
                   </children>
               </AnchorPane>
           </content>
